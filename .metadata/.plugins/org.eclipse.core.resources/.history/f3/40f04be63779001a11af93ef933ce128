import java.util.Scanner;

public class test {

    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        
        int[][] array = {{1, 2}, {3, 4}, {5, 6}}; 
        int sum = 0; 
        for (int i = 0; i < array.length; i++) 
               sum += array[i][0]; 
        System.out.println(sum);
    }

}