public class test {
    static String global=""; // part A
    public static void main(String[] args) { 
 
     global="Yum!";
 
 
     System.out.println(global); // part B
     myMethod("Pizza");
 }
 
 public static void myMethod (String dish){
 
     global="Cheesey"; // part C
     System.out.println(global+" "+dish); // part D
 }
 
}