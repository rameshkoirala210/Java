		 arr[0][2] = a.nextInt();
		 
		 for (int s = 0; s < 3; s++) {
	            System.out.println("Student number " + (s + 1) );
	        for (int i = 0; i < count; i++) {
	              System.out.println("Enter score no." + (i + 1) + " : ");
	               grade = a.nextInt();
	        }
	        System.out.println("The average is " + grade);
	            
	            //		 for (int row = 0; row < arr.length ; row++)
