import java.util.*;

public class lab09A2 {
	 public static void main(String[] args){ 
		 Scanner a = new Scanner(System.in);
		 
		 	System.out.println("Enter how many rows would like in the multiplication table: ");
			int rows = a.nextInt();
			System.out.println("Enter how many columns would like in the multiplication table: ");
			int columns = a.nextInt(); 
			
	        int[][] arr = new int[rows][columns]; 
	        
	        for (int row = 0; row < arr.length ; row++)
	        {
	            for (int column = 0; column < arr[row].length; column++)
	            {
	                arr[row][column] = (row+1)*(column+1);
	            }

	        }
	 }
}