import java.io.File;
import java.io.PrintWriter;
import java.util.Scanner;
import java.io.IOException;

public class writer {
  public static void main(String[] args) throws IOException {
	  Scanner a = new Scanner(System.in);
	  File file = new File("accounts.txt");
	  PrintWriter fo = new PrintWriter(file);
	  String choice = "";
	  int num = 0;
	  	do {
	  System.out.println("jsdk ");
	   choice = a.next();	
	   fo.write(choice + "\n");   
	  	}while(!(choice.equals("done")));
	  	
	  	File  nu= new File("accounts1.txt");
		PrintWriter fo1 = new PrintWriter(file);
	  	do {
	  	  System.out.println("jsdk1 ");
	  	  num = a.nextInt();	
	  	  fo1.write(num + "\n");   
	  	 }while(num != 0);
		
	  	a.close();
		fo1.close();
		  
		
  }
}